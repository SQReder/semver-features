{
  "name": "semver-features-cli",
  "version": "0.0.0",
  "description": "SemVer-Based Feature Toggle Library - CLI Package",
  "sideEffects": false,
  "private": true,
  "files": [
    "dist"
  ],
  "bin": {
    "semver-features": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:package": "publint .",
    "lint:attw": "attw --pack . --profile esm-only",
    "prepublishOnly": "npm run build && npm run lint:package && npm run lint:attw"
  },
  "keywords": [
    "semver",
    "feature-toggle",
    "feature-flag",
    "versioning"
  ],
  "author": "SQReder <sqreder@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.25",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "typescript": "^5.0.3",
    "tsup": "^6.0.0",
    "@repo/typescript-config": "*",
    "@repo/tsup-config": "*"
  },
  "dependencies": {
    "semver-features-json": "*",
    "enquirer": "^2.4.1",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "cosmiconfig": "^8.3.6",
    "chalk": "^4.1.2",
    "date-fns": "^3.3.1",
    "fs-extra": "^11.2.0",
    "semver": "^7.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SQReder/semver-features.git"
  },
  "bugs": {
    "url": "https://github.com/SQReder/semver-features/issues"
  },
  "homepage": "https://github.com/SQReder/semver-features"
}