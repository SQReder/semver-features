{
  "name": "semver-features-react",
  "version": "4.0.0",
  "description": "SemVer-Based Feature Toggle Library - React Integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:package": "publint .",
    "lint:attw": "attw --pack . --profile esm-only",
    "prepublishOnly": "npm run build && npm run lint:package && npm run lint:attw"
  },
  "keywords": [
    "semver",
    "feature-toggle",
    "feature-flag",
    "versioning",
    "react"
  ],
  "author": "SQReder <sqreder@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.37.0",
    "jsdom": "^24.0.0",
    "react": "^17.0.2",
    "typescript": "^5.0.3",
    "@repo/typescript-config": "*",
    "@repo/tsup-config": "*"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "semver-features": "^3.1.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "dependencies": {
    "semver-features": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SQReder/semver-features-react.git"
  },
  "bugs": {
    "url": "https://github.com/SQReder/semver-features-react/issues"
  },
  "homepage": "https://github.com/SQReder/semver-features-react"
}
